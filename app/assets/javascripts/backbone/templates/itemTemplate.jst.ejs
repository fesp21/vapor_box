<div class="plan-header"><%= item.get('name') %></div>
  <% if (item.get('type') == 'plan')  { %>
    <div class="plan-image">
      <img src="<%= item.get('image') %>" alt="<%= item.get('name') %> Image">
  <% } else { %>
    <div class="plan-image" style="background: url('<%= item.get('image') %>') no-repeat center center; background-size: 80%">
  <% } %> 
  </div>


  <div class="plan-description">
    <%= item.get('description') %>
  </div>
  <% if (item.get('type') != 'flavor')  { %>
  <div class="plan-cost" >
    $<%= (item.get('cost')/100).toFixed(2) %>
  </div>
  <% }%>
  <% if (item.get('type') == 'flavor')  { %>
    <div class='select-container' data-name="<%= item.get('name') %>">
  
  <% } else { %>
    <div class='select-container' data-name="<%= item.get('uniqueId') %>">
  <% }%>
</div>  


