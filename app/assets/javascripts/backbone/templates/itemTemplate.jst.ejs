<div class="plan-header"><%= item.get('name') %></div>
<div class="plan-image">
  <img src="<%= item.get('image') %>" alt="<%= item.get('name') %> Image">
</div>
<div class="plan-description">
  <%= item.get('description') %>
</div>
<div class="plan-cost" ><%= item.get('cost') %></div>
<% if (item.get('selected')) { %>
  <div class='selected-buttons'>
  Selected

  <% if (item.get('type') != 'plan') { %>
    <i class='fa fa-minus remove-quantity'></i>
  
  <% } %>
  </div>
<% } %>